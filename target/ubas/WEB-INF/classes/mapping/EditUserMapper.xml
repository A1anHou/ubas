<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.alan.dao.EditUserDao">

    <insert id="insertEditRecord">
        INSERT INTO edit_user(user_id,user_attr,user_old,user_new,user_change_time)
        VALUES (#{userId},#{userAttr},#{userOld},#{userNew},#{userChangeTime})
    </insert>
</mapper>