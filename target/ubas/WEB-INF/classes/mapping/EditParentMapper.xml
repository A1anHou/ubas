<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.alan.dao.EditParentDao">

    <insert id="insertEditRecord">
        INSERT INTO edit_parent(parent_id,parent_attr,parent_old,parent_new,parent_change_time)
        VALUES (#{parentId},#{parentAttr},#{parentOld},#{parentNew},#{parentChangeTime})
    </insert>
</mapper>