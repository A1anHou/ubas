<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.alan.dao.EditRelationDao">

    <insert id="insertEditRecord">
        INSERT INTO edit_relation(relation_id,parent_id,user_id,relation_attr,relation_old,relation_new,relation_change_time)
        VALUES (#{relationId},#{parentId},#{userId},#{relationAttr},#{relationOld},#{relationNew},#{relationChangeTime})
    </insert>
</mapper>